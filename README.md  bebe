  <img src="    import boto3

# Configura tus credenciales de AWS
s3 = boto3.client('s3')

# Ruta local del archivo a subir
archivo_local = 'ruta/al/archivo.txt'
# Nombre del bucket y nombre del archivo en la nube
bucket = 'mi-bucket'
archivo_en_nube = 'carpeta/archivo.txt'

# Subir archivo a S3
s3.upload_file(archivo_local, bucket, archivo_en_nube)
print("Archivo subido exitosamente a la nube.")
